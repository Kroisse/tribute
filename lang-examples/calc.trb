(fn (main)
    (let line (split " " (trim_right (input_line))))
    (let a (to_number (get 0 line)))
    (let op (get 1 line))
    (let b (to_number (get 2 line)))
    (let result (match op
        (case "+" (+ a b))
        (case "-" (- a b))
        (case "*" (* a b))
        (case "/" (/ a b))))
    (print_line result))
